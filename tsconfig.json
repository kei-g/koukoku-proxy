{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "declarationDir": ".",
    "downlevelIteration": true,
    "emitBOM": false,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": [
      "ES2021.String",
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "newLine": "lf",
    "pretty": false,
    "removeComments": false,
    "strict": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types",
    ],
  },
  "exclude": [],
  "include": [
    "src",
  ],
}
